<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//En" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Board">

<!-- 목록 보기 -->
<select id="boardList" parameterType="board" resultType="board">
SELECT * FROM BOARD
</select>


<insert id="boardWrite" parameterType="board">
insert into board values(#{board_num},#{board_name},#{board_pass},#{board_subject},
#{board_content},#{board_file},#{board_re_ref},#{board_re_lev},#{board_re_seq},#{board_readcount},
#{date})
</insert>

</mapper>